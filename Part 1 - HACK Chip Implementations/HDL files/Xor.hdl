/**
 * Exclusive-or gate:
 * out = not (a == b)
 */

CHIP Xor {
    IN a, b;
    OUT out;

    PARTS:
	Nand(a=a,b=abOut,out=aNandOut);
	Nand(a=abOut,b=b,out=bNandout);
	Nand(a=a,b=b,out=abOut);
	Nand(a=aNandOut,b=bNandout,out=out);
}